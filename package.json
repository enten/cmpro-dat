{
  "name": "cmpro-dat",
  "version": "0.0.3",
  "description": "ClrMamePro DAT parser for Node.js.",
  "license": "MIT",
  "author": "Steven Enten <steven@enten.fr>",
  "keywords": [
    "clrmamepro",
    "dat",
    "parser"
  ],
  "homepage": "https://github.com/enten/cmpro-dat",
  "bugs": {
    "url": "https://github.com/enten/cmpro-dat/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enten/cmpro-dat"
  },
  "main": "cmpro-dat.js",
  "typings": "cmpro-dat.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc --project ./tsconfig.lib.json",
    "prebuild": "rm -fr ./dist",
    "postbuild": "cp ./package.dist.json ./dist/package.json && cp ./LICENSE ./README.md ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:quick": "pretty-quick"
  },
  "devDependencies": {
    "@types/jest": "~29.1.0",
    "@types/node": "~18.7.23",
    "husky": "~8.0.1",
    "jest": "~29.1.1",
    "prettier": "~2.7.1",
    "pretty-quick": "~3.1.3",
    "ts-jest": "~29.0.3",
    "tslib": "~2.4.0",
    "typescript": "~4.8.4"
  }
}
